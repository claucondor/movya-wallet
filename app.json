{
  "expo": {
    "name": "Movya Wallet",
    "slug": "movya-wallet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo/logo@SD.png",
    "scheme": "exp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.oydual3org.movyawallet",
      "usesAppleSignIn": true,
      "associatedDomains": ["webcredentials:<your-associated-domain>"],
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/logo@HD.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.oydual3org.movyawallet"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/logo/logo@favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-apple-authentication",
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "17.5"
          },
          "android": {
            "compileSdkVersion": 34
          }
        }
      ],
      "expo-font",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "3a77727f-bb1c-4cf3-be70-426deef98387"
      },
      "privyAppId": "cm8t09wte002f4624595l3wba",
      "privyClientId": "client-WY5i4S9CLNPYiaRbuA5s8wfAwYdM5LhSBsC2nwPdDebU4"
    },
    "owner": "oydual3-org",
    "updates": {
      "url": "https://u.expo.dev/3a77727f-bb1c-4cf3-be70-426deef98387"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
